    "def sc (x):\n",
    "    return x**3*sin(x)-cos(x)\n",
    "x = np.linspace(0,7,1000)\n",
    "\n",
    "plt.figure(figsize = (8, 6));\n",
    "plt.grid(color = \"k\", linewidth = 0.2)\n",
    "#plt.plot(x, sc(x),\"y--\", linewidth = 3);\n",
    "#red  graph is natural spline\n",
    "#blue graph is not a knot spline\n",
    "plt.plot(x,eval_xtab(np.linspace(0,7,8),spline_coeff(sc,np.linspace(0,7,8),\"Nat\"),x,1000) ,\"r--\",linewidth = 3.1);\n",
    "plt.plot(x,eval_xtab(np.linspace(0,7,8),spline_coeff(sc,np.linspace(0,7,8),\"naK\"),x,1000) ,\"b--\",linewidth = 3.1);\n",
    "plt.plot(np.linspace(0,7,8), sc(np.linspace(0,7,8)), \"o\", markersize = 10,\n",
    "         markerfacecolor = \"darkorange\", markeredgecolor = \"rebeccapurple\",markeredgewidth = 2.3);\n",
    "\n",
    "infinite_norm1 = max(abs(sc(x) - eval_xtab(np.linspace(0,7,8),spline_coeff(sc,np.linspace(0,7,8),\"Nat\"),x,1000)))\n",
    "infinite_norm2 = max(abs(sc(x) - eval_xtab(np.linspace(0,7,8),spline_coeff(sc,np.linspace(0,7,8),\"naK\"),x,1000)))\n",
    "plt.xlabel(f\"∞ norm (Nat) = {infinite_norm1:.3f}, ∞ norm (naK) = {infinite_norm2:.3f}\", fontsize = 15);"
