    "import scipy as sp\n",
    "from scipy import special\n",
    "from scipy import linalg\n",
    "\n",
    "def divdiff (g, X): \n",
    "    A = g(X).copy()\n",
    "    for i in range (1,len(X)):\n",
    "        for j in range(len(X)-1,i-1,-1):\n",
    "            A [j] = (A[j]-A[j-1])/(X[j]-X[j-i])\n",
    "    return(A)\n",
    "\n",
    "def spline_coeff (f, X, tys):\n",
    "    \"\"\"\n",
    "    Outputs coefficients of the polynomials interpolating the function.\n",
    "    Takes as inputs f function, X vector (interpolation points), tys (string\n",
    "    defining the type of cubic spline)\n",
    "    \"\"\"\n",
    "    \n",
    "    # n=number of intervals\n",
    "    F = f(X)\n",
    "    n = len(X)-1\n",
    "    #distance between interpolation points\n",
    "    H = np.zeros(n)\n",
    "    H[::] = X[1::] - X[:n:]\n",
    "    #\n",
    "    ##\n",
    "    \n",
    "    Dpos = np.array([])\n",
    "    Dneg = np.array([])\n",
    "    for i in range(1,n):\n",
    "        Dpos = np.append(Dpos,H[i-1]/(H[i]+H[i-1]))\n",
    "        Dneg = np.append(Dneg,H[i]/(H[i]+H[i-1]))\n",
    "    #upper and lower diagonals\n",
    "    #Dpos = H[1:n-1:]/(H[:n-2:]+H[1:n-1:])\n",
    "    #Dneg = H[:n-2:]/(H[:n-2:]+H[1:n-1:])\n",
    "    #print(Dpos,Dneg)\n",
    "    \n",
    "    if tys==\"Nat\":\n",
    "        A = np.eye(n-1,n-1)*2 + np.diag(Dneg[:-1:], k=-1) + \\\n",
    "        + np.diag(Dpos[1::], k=1)\n",
    "        #K known term vector\n",
    "        #print(A)\n",
    "        K = np.zeros(n-1)\n",
    "        for i in range (n-1):\n",
    "            K [i] = 6 * (divdiff (f, np.array([X[i],X[i+1],X[i+2]]))[-1])\n",
    "        \n",
    "        #np.reshape(K, (n-1,1))\n",
    "        Mi = linalg.solve(A, K)\n",
    "        #print(Mi)\n",
    "        #print(A*Mi-K)\n",
    "        M0 = np.array([0])\n",
    "        #M momenti vector\n",
    "        M = np.concatenate((M0,Mi,M0))\n",
    "        #coefficient of polynomial in each interval\n",
    "        coeff = (np.array([]),np.array([]),np.array([]),np.array([]))\n",
    "        '''\n",
    "        coeff = (M[1:]/6*H[::], \\\n",
    "                 M[:-1:]/6*H[::], \\\n",
    "                 (F[1::]-F[:-1:])/H[::]-(H[::]*(M[1::]-M[:-1:]))/6, \\\n",
    "                 F[:-1:]-H[::]**2*M[:-1:]/6)\n",
    "              '''\n",
    "        for k in range(1,n+1):\n",
    "            coeff = (np.append(coeff[0],M[k]/(6*H[k-1])), \\\n",
    "                     np.append(coeff[1],M[k-1]/(6*H[k-1])), \\\n",
    "                     np.append(coeff[2],(F[k]-F[k-1])/H[k-1]-(H[k-1]*(M[k]-M[k-1]))/6), \\\n",
    "                     np.append(coeff[3], F[k-1]-H[k-1]**2*M[k-1]/6))     \n",
    "        return coeff\n",
    "\n",
    "    if tys==\"naK\":\n",
    "        Dpos = np.append(np.array([0]),Dpos)\n",
    "        Dneg = np.append(Dneg,np.array([0]))\n",
    "        B = np.eye(n+1)\n",
    "        B = 2*B\n",
    "        B[0][0], B[-1][-1] = 1, 1\n",
    "        B = B+ np.diag(Dneg[::], k=-1) + \\\n",
    "        + np.diag(Dpos[::], k=1)\n",
    "        #K known term vector\n",
    "        K = np.zeros(n+1)\n",
    "        for i in range (n-1):\n",
    "            K [i+1] = 6 * (divdiff (f, np.array([X[i],X[i+1],X[i+2]]))[-1])\n",
    "        K[0] = K[1]\n",
    "        K[n] = K[n-1]\n",
    "    \n",
    "        B[1][1] = 2 - Dneg[0]\n",
    "        B[1][2] = Dpos [0]- Dneg[0]\n",
    "        B[-2][-2] = 2 - Dpos[-1]\n",
    "        B[-2][-3] = Dneg[-1] - Dpos[-1]\n",
    "        #M momenti vector\n",
    "        M = linalg.solve(B, K)\n",
    "    \n",
    "        #M0 = np.array([((H[0]+H[1])*Mi[0]/H[1]) - (H[0]*Mi[1]/H[1])])\n",
    "        #Mn = np.array([(Mi[-1]*(H[-1]+H[-2])/(H[-2]-H[-1]/H[-2]*Mi[n-2]))])\n",
    "        #M = np.concatenate((M0,Mi,Mn))\n",
    "        \n",
    "        #coefficient of polynomial in each interval\n",
    "        coeff = (M[1:]/6*H[::], \\\n",
    "                 M[:-1:]/6*H[::], \\\n",
    "                 (F[1::]-F[:-1:])/H[::]-(H[::]*(M[1::]-M[:-1:]))/6, \\\n",
    "                 F[:-1:]-H[::]**2*M[:-1:]/6)\n",
    "        return coeff  "
   ]
