   "source": [
    "def eval_xtab (knots, coeff, xtab, ntab):\n",
    "    #assert min nodo<ogni xtab<max nodo\n",
    "    start = 0\n",
    "    stab = np.zeros(ntab)\n",
    "    for j in range(ntab):\n",
    "        for i in range(start, len(knots)-1):\n",
    "            if (knots[i]<= xtab[j] <= knots[i+1]):\n",
    "                stab[j]=(coeff[0][i]*(xtab[j]-knots[i])**3) + (coeff[1][i]*(knots[i+1]-xtab[j])**2) \\\n",
    "                + coeff[2][i]*(xtab[j]-knots[i]) + coeff[3][i]\n",
    "                start = i\n",
    "                break\n",
    "\n",
    "    return stab          "
   ]
  },
