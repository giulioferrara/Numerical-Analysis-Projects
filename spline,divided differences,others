   "cell_type": "code",
   "execution_count": 828,
   "id": "80e3333a-89f6-4218-8206-0cf18018a408",
   "metadata": {},
   "outputs": [],
   "source": [
    "def secants (f, x0, x1, tol, maxiter):\n",
    "    '''\n",
    "    approximates zeros of a function \n",
    "    using secants method\n",
    "    '''\n",
    "    for i in range (maxiter):\n",
    "        if f(x1) == f(x0):\n",
    "            print (\"Not possible to execute iteration\")\n",
    "            break\n",
    "        x2 = x1 - ((x1-x0)*f(x1)/(f(x1)-f(x0)))\n",
    "        x0, x1 = x1, x2\n",
    "        if abs((x1-x0)/(1+x1)) <= tol:\n",
    "            break\n",
    "     \n",
    "    return (x1,i)"
   ]
  },
