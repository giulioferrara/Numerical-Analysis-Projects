    "#defining functions\n",
    "def f1 (x):\n",
    "    return x**3+3*x+1\n",
    "    \n",
    "def f2(x):\n",
    "    return np.sin(np.pi * x)\n",
    "    \n",
    "def f3(x):\n",
    "    return np.sqrt(1+x**2)\n",
    "    \n",
    "def f4 (x):\n",
    "    return abs(x**3)+np.cos(np.pi * x)\n",
    "    \n",
    "#defining second derivatives of the functions\n",
    "def ddf1(x):\n",
    "    return 6*x\n",
    "    \n",
    "def ddf2(x):\n",
    "    return -np.sin(np.pi*x)*(np.pi**2)\n",
    "    \n",
    "def ddf3(x):\n",
    "    return 1/(1+x**2)**(3/2)\n",
    "    \n",
    "def ddf4(x):\n",
    "    return 6*abs(x)-(np.pi**2)*np.cos(np.pi*x)\n",
    "\n",
    "derivatives = [\n",
    "    ddf1(np.array([0,0.5,1])),\n",
    "    ddf2(np.array([0,0.5,1])),\n",
    "    ddf3(np.array([0,0.5,1])),\n",
    "    ddf4(np.array([0,0.5,1]))\n",
    "]\n",
    "\n",
    "#l = point where we approximate second derivative   \n",
    "l = np.array ([0,0.5,1])\n",
    "\n",
    "fun = [f1,f2,f3,f4]\n",
    "\n",
    "#titles\n",
    "print (\"|Funzione\\t| h\\t\\t|\\t\\tApprossimazione\\t\\t\\t|\\t\\tErrore\\t\\t\\t |\")\n",
    "print (\"-\" * 122)\n",
    "\n",
    "\n",
    "for j in range(4):\n",
    "    for i in range (0,10):\n",
    "        h = 2 * (10 ** (-i - 1))\n",
    "        t = approxf2(l, 2*(10**(-i+1)),fun[j])\n",
    "        #converting values in strings\n",
    "        e = abs(approxf2(l,2*(10**(-i+1)),fun[j])- derivatives[j])\n",
    "        t_str = \"\\t\".join([f\"{val:.8f}\" for val in t])\n",
    "        e_str = \"\\t\".join([f\"{val:.8f}\" for val in e])\n",
    "        #formatting in columns\n",
    "        print(f\"{fun[j].__name__}\\t\\t {h:.10f}\\t {t_str}\\t {e_str}\")\n",
    "        \n",
    "        # Converti i valori in stringhe\n",
    "        #t_str = \"  \".join([str(val).ljust(30) for val in t])\n",
    "        #e_str = \"  \".join([str(val).ljust(30) for val in e])\n",
    "\n",
    "        # formattazione in colonne\n",
    "        #print(f\"{fun[j].__name__.ljust(10)}{str(h).ljust(10)}{t_str}{e_str}\")\n",
    "       \n",
    "    \n"
   ]
