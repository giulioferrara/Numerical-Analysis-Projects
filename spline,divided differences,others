    "import numpy as np\n",
    "    \n",
    "def approxf2 (x, h, f):\n",
    "    '''\n",
    "    returns the approximations of the second derivative of the function f\n",
    "    in the entries of the vector x using the method of\n",
    "    finite differences on h\n",
    "    '''\n",
    "    assert isinstance (x, np.ndarray)\n",
    "    assert isinstance (h, float|int)\n",
    "    assert (h>0)\n",
    "    assert callable(f)\n",
    "    \n",
    "    a = np.zeros(len(x))\n",
    "    a = (+4*f(x[:]+2*h)+2*f(x[:]))/(h**2) - (f(x[:]+3*h)+5*f(x[:]+h))/(h**2)\n",
    "    return a\n",
    "\n",
    "\n",
    "\n"
   ]
  },
